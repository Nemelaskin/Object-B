@using System.Linq;
@{
    Layout = null;

}
<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <link rel="stylesheet" type="text/css" href="~/css/CMapStyle.css" />
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <script src="~/js/CMapJS.js" defer></script>

    <title>Test</title>
</head>

<body>
    <header>
        <nav>
            <ul class="topmenu">
                <li><a href="">@ViewBag.Select</a></li>
                <li>
                    <a href="" class="submenu-link">Companies</a>
                    <ul class="submenu">
                        @foreach (var i in ViewBag.Companies)
                        {
                            <li><a href="">@i</a></li>

                        }
                    </ul>
                </li>
                <li><a asp-controller="MapComp" asp-action="Index">Maps</a></li>
            </ul>
        </nav>
    </header>


    <h1>Красный квадрат = 1м^2</h1>
    <div id="wrapper">
        <div id="content">
            <div id="places"></div>

        </div>
        <div id="sidebar"></div>
    </div>
    <div class="SelectRoom">
        <h2>Select Room to add coordinates:</h2>
        
    </div>

    <div class="test">
        <button name="send" onclick="newTestButt()">Send!</button>
    </div>
</body>
</html>