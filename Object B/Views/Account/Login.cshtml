@{

    ViewData["Title"] = "Login";
}

<h1>Login</h1>

<form action="Login" method="post">

    <div class="form-group">
        <label for="2">Phone</label>
        <input name="phone" type="tel" class="form-control" id="2" />
    </div>

    <div class="form-group">
        <label for="33">Password</label>
        <input name="password" type="password" class="form-control" id="33" />
    </div>
    <input type="submit" value="Отправить" class="btn btn-primary" />
</form>

